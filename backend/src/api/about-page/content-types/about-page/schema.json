{
  "kind": "singleType",
  "collectionName": "about_pages",
  "info": {
    "singularName": "about-page",
    "pluralName": "about-pages",
    "displayName": "About Page",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "breadcrumb": {
      "type": "component",
      "repeatable": false,
      "component": "shared.breadcrumb",
      "required": true
    },
    "title": {
      "type": "string"
    },
    "heading": {
      "type": "text",
      "required": true
    },
    "description": {
      "type": "text",
      "required": true
    },
    "button": {
      "type": "component",
      "repeatable": false,
      "component": "shared.button",
      "required": true
    },
    "hero_image": {
      "type": "media",
      "multiple": false,
      "required": true,
      "allowedTypes": [
        "images"
      ]
    },
    "images": {
      "type": "component",
      "repeatable": true,
      "component": "shared.image-lists",
      "max": 2
    },
    "team": {
      "type": "component",
      "repeatable": false,
      "component": "components.team",
      "required": true
    },
    "brand": {
      "type": "component",
      "repeatable": false,
      "component": "components.brand",
      "required": true
    },
    "seo": {
      "type": "component",
      "repeatable": false,
      "component": "shared.seo",
      "required": true
    }
  }
}
